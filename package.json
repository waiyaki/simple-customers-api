    "sqlite3": "^4.0.3",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.5",
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.4",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.6.4"
  }
}
